<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
  version="3.1"
  metadata-complete="true">
  
<servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>InsertData</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/home</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ContactUs</servlet-name>
    <servlet-class>Contact</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>ContactUs</servlet-name>
    <url-pattern>/contactus</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>Booking</servlet-name>
    <servlet-class>Booking</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>Booking</servlet-name>
    <url-pattern>/booking</url-pattern>
</servlet-mapping>
 
 <servlet>
    <servlet-name>Validate</servlet-name>
    <servlet-class>Validate</servlet-class>
</servlet>


<servlet-mapping>
    <servlet-name>Validate</servlet-name>
    <url-pattern>/homepage</url-pattern>
</servlet-mapping>
</web-app>
